<div class="search-box__offer-content">
    <div class="search-box__offer-top">
        <div class="search-box__offer-top-wrap">
            <div class="search-box__offer-title">
                <span>{{ cardName }}</span>
                <span class="blue"> ({{ category }}) <strong>{{ [multiplier, 'x']|join }}</strong></span>
            </div>
        </div>
    </div>
    <div class="modal">
        <p>
            {{ 'credit-card.offer.popup.merchant-reverse'|trans({
                '%strong_on%': '<strong>',
                '%strong_off%': '</strong>',
                '%multiplier%': [multiplier, 'x']|join,
                '%category%': category
            })|desc("Currently, the following merchants earn %strong_on%%multiplier%%strong_off% at %strong_on%%category%%strong_off%:") | raw }}
        </p>
        <div class="container">
            {% for merchant in merchants %}
                <div class="container__row"> <!-- .show-details -->
                    <a href="#" class="container__link">
                        {{ merchant.name }}
                    </a>

                    {% if merchant.knownCategories is defined %}
                        <div class="container__details">
                            <p>{{ merchant.name }} has been seen to be coded as:</p>
                            <div class="search-box__offer-top-container">

                                {% for provider, categories in merchant.knownCategories %}
                                <div class="search-box__offer-top-container__row">
                                    <div class="search-box__offer-top-container__col">by {{ provider }}:</div>
                                    <div class="search-box__offer-top-container__col">
                                        <ul class="check-list">
                                            {% for category in categories %}
                                                <li>{{ category.name|raw }} (<span class="red">{{ category.percentage }}%</span>)</li>
                                            {% endfor %}
                                        </ul>
                                    </div>
                                </div>
                                {% endfor %}

                            </div>
                        </div>
                    {% endif %}
                </div>
            {% endfor %}
        </div>
        <div class="credit-card-info">
            <a href="{{ blogUrl }}" target="_blank">
                {{ 'credit-card.offer.popup.merchant.offer'|trans({
                    '%up-counter-on%': '<span class="up-counter">',
                    '%up-counter-off%': '</span>',
                    '%blue-text-on%': '<span class="blue-text">',
                    '%blue-text-off%': '</span>',
                    '%potential%': [multiplier, 'x']|join,
                    '%category%': category
                })|desc('How to Earn up to %up-counter-on%%potential%%up-counter-off% points per dollar at %blue-text-on%%category%%blue-text-off%*') | raw }}
            </a>
        </div>
        <div class="remark-info small">
            <i>
                {{ 'credit-card.offer.popup.remark.info'|trans({
                    '%advertisers-link-begin%': '<a target="_blank" href="https://awardwallet.com/blog/advertiser-disclosure/">',
                    '%advertisers-link-end%': '</a>'
                })| raw }}
            </i>
        </div>
    </div>
</div>
{% endblock %}
