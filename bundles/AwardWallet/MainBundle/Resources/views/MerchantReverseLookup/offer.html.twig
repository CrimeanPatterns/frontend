        height: 40px;
        line-height: 40px;
        position: relative;
        text-decoration: none;
        font-size: 13px;
        color: #535457 !important;
        font-weight: normal !important;
        padding-left: 20px;
        border-bottom: 1px dotted #8e9199;
        white-space: nowrap;
        overflow: hidden;
        text-overflow: ellipsis;
    }
    .container__link:hover{
        text-decoration: none !important;
        color: #4684c4 !important;
    }
    .container__link:after{
        position: absolute;
        content: "";
        width: 9px;
        height: 6px;
        transform: rotate(-90deg);
        background-image: url(https://awardwallet.com/assets/awardwalletnewdesign/img/sprite.png?v=18);
        background-repeat: no-repeat;
        background-position: -32px -66px;
        pointer-events: none;
        display: block;
        left: 4px;
        top: 50%;
        margin-top: -3px;
        -webkit-transition: transform .5s ease;
        -moz-transition: transform .5s ease;
        -o-transition: transform .5s ease;
        transition: transform .5s ease;
    }
    .container__row.show-details .container__details{
        max-height: 9999px;
        overflow: visible;
        padding: 20px 20px 8px;
        border-bottom: 1px dotted #8e9199;
    }
    .container__row.show-details .container__link:after{
        transform: rotate(0deg);
    }
    .container__details{
        overflow: hidden;
        max-height: 0;
