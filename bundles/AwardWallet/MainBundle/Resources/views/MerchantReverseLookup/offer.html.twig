{% extends app.request.query.has('noBody') ? "@AwardWalletMain/MerchantReverseLookup/noBody.html.twig" : "@AwardWalletMain/MerchantReverseLookup/body.html.twig" %}

{% block javascripts %}
    {{ parent() }}
    <script>
        setTimeout(function () {
            var merchants = document.getElementsByClassName('container__link');
            for (var i = 0; i < merchants.length; i++) {

                merchants[i].addEventListener('click', function (evt) {
                    evt.preventDefault();
                    this.parentElement.classList.toggle("show-details");
                    if (window.hasOwnProperty('sendClientHeight')) {
                        window.sendClientHeight();
                    }
                    return false;
                });
            }
        }, 500);
    </script>
{% endblock %}
