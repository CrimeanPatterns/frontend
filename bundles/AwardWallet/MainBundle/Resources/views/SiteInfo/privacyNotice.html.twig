<script>
{% if is_granted('SITE_MOBILE_APP') %}
		$('.scroll-links span[data-href]')
				.prop('onclick', null)
				.off('click')
				.on('click.scroll', function(e) {
					e.preventDefault();
					var $section = $($(e.target).data('href'));
					if ($section.length === 0)
						return false;
					$section.get(0).scrollIntoView();
				});
{% else %}
	{% if not is_granted("SITE_MOBILE_AREA") %}
		require(['jquery-boot'], function($) {
	{% endif %}
			if(typeof $ !== 'undefined') {
				$('.scroll-links span[data-href]')
						.prop('onclick', null)
						.off('click')
						.on('click.scroll', function (e) {
							e.preventDefault();
							var hash = $(e.target).data('href').substr(1);
							var $section = $('#' + hash);
							if ($section.length === 0)
								return false;

							$('html,body').animate({scrollTop: $section.offset().top - 10}, 500);
							location.hash = hash;
							return false;
						});
			}
	{% if not is_granted("SITE_MOBILE_AREA") %}
	});
	{% endif %}
{% endif %}
</script>
