{% trans_default_domain "privacy" %}

{% set queryExtend = [] %}
{% set contentLinkClick = ' onclick="location.hash = this.getAttribute(\'data-href\'); return false;"' %}
{% if is_granted('SITE_MOBILE_APP') %}
    {% set queryExtend = { fromapp : 1, KeepDesktop: 1 } %}
    {% set contentLinkClick = '' %}
{% endif %}

	<div class="main-blk">
		{% if heading is defined %}{{ heading|raw }} {% endif %}
		<!-- mobile native app style -->
		<style>
			.wrapper .content.static-page .main-blk ul li:after{
				background: #535457;
			}
			.wrapper .content.static-page .main-blk ul{
				background: transparent !important;
			}
			.wrapper .content.static-page .main-blk .number-list li{
				padding-left: 20px;
			}
			.wrapper .content.static-page .main-blk .number-list li:first-child{
				margin-top:10px;
			}
			.content .scroll-links a[data-href],
			.content .scroll-links span[data-href] {
				font-size: 16px;
				text-decoration: none;

				font-weight: bold;
				color: #4684c4;
				cursor: pointer;
				font-family: 'open_sansbold', 'open_sansregular', sans-serif;
			}
			.wrapper .content.static-page {
				background: #fff;
			}
			.wrapper .content.static-page h4 {
				font-size: 20px;
				line-height: 1.3;
				color: #535457;
			}
			.wrapper .content.static-page section {
				padding-top: 30px;
			}
			.wrapper .content.static-page section:first-child {
				padding-top: 0;
			}
			.wrapper .content.static-page .privacy-table{
				display: block;
				border-top: 2px solid #bec2cc !important;
			}
			.wrapper .content.static-page .privacy-table th{
				display: none;
			}
			.wrapper .content.static-page .privacy-table td,
			.wrapper .content.static-page .privacy-table tr{
				display: block;
			}
			.wrapper .content.static-page .privacy-table tr{
				border-top: 2px solid #bec2cc;
				margin-top: -1px;
			}
			.wrapper .content.static-page .privacy-table tr:first-child{
				border-top: 0;
			}
			.wrapper .content.static-page .privacy-table td{
				display: flex;
				flex-direction: row;
				flex-wrap: nowrap;
				align-items: center;
				padding-left: 0;
				padding-right: 0;
			}
			.wrapper .content.static-page .privacy-table td:before{
				content: attr(data-title);
				font-size: 13px;
				color: #8e9199;
				display: block;
				width: 100px;
				padding-right: 10px;
			}
			.wrapper .content.static-page .privacy-table td span{
				width: calc(100% - 100px);
			}
			.main-blk-content > section[id] {
				padding-top:0;
			}
			.main-blk-content > section[id] h4 {
				padding-top: 20px;
			}
			{% if is_granted('SITE_MOBILE_AREA') %}
				@font-face {
		            font-family: 'Open Sans', sans-serif;
		            src: url('https://awardwallet.com/assets/awardwalletnewdesign/font/opensans-regular-webfont.eot?v=2');
		            src: url('https://awardwallet.com/assets/awardwalletnewdesign/font/opensans-regular-webfont.eot?#iefix') format('embedded-opentype'),
		                 url('https://awardwallet.com/assets/awardwalletnewdesign/font/opensans-regular-webfont.woff?v=2') format('woff'),
		                 url('https://awardwallet.com/assets/awardwalletnewdesign/font/opensans-regular-webfont.ttf?v=2') format('truetype');
		            font-weight: normal;
		            font-style: normal;
		        }

		        @font-face {
		            font-family: 'Open Sans', sans-serif;
		            src: url('https://awardwallet.com/assets/awardwalletnewdesign/font/opensans-bold-webfont.eot?v=2');
		            src: url('https://awardwallet.com/assets/awardwalletnewdesign/font/opensans-bold-webfont.eot?#iefix') format('embedded-opentype'),
		                 url('https://awardwallet.com/assets/awardwalletnewdesign/font/opensans-bold-webfont.woff?v=2') format('woff'),
		                 url('https://awardwallet.com/assets/awardwalletnewdesign/font/opensans-bold-webfont.ttf?v=2') format('truetype');
		            font-weight: 700;
		            font-style: normal;
		        }

		        body,html{
		            margin: 0;
		            padding: 0;
		            font-family: 'Open Sans', sans-serif;
		        }

		        * {
		            margin:0;
		            padding: 0;
		            box-sizing: border-box;
		        }

		        .main-blk {
		            font-family: 'Open Sans', sans-serif;
		        }

		        .main-blk h1,
		        .main-blk h2,
		        .main-blk h3,
		        .main-blk h4{
		            font-weight: normal;
		        }

		        .main-blk .bold,
		        .main-blk strong {
		            font-weight: bold;
		        }

		        .main-blk p {
		            font-size: 16px;
		            color: #535457;
		            padding-top: 20px;
		            line-height: 1.625;
		        }

		        .main-blk p:empty {
		            display: none
		        }

		        .main-blk p span.red {
		            color: #cc3d5e;
		            font-weight: 700
		        }

		        .main-blk p:first-child {
		            padding-top: 0
		        }

		        .main-blk h2 {
		            display: block;
		            margin: -15px -15px 0;
		            padding: 15px;
		            background: #f8f9fa;
		            position: relative;
		            font-size: 20px;
		            color: #5c6373;
		            text-align: center;
		            border-bottom: 1px solid #dfe1e6
		        }

		        .main-blk h3 {
		            display: block;
		            margin: 10px -15px 0;
		            padding: 30px 15px 10px;
		            background: #fff;
		            position: relative;
		            font-size: 20px;
		            color: #5c6373;
		            border-bottom: 2px solid #bec2cc;
		            border-top: 1px solid #dfe1e6
		        }

		        .main-blk h4 {
		            font-size: 20px;
		            line-height: 1.3;
		            color: #535457
		        }

		        .main-blk section {
		            padding-top: 30px
		        }

		        .main-blk section:first-child {
		            padding-top: 0
		        }

		        .main-blk .row-blk {
		            margin-top: 45px
		        }

		        .main-blk .row-blk:first-child {
		            margin-top: 25px
		        }

		        .main-blk .row-blk:last-child {
		            margin-bottom: 5px
		        }

		        .main-blk .row-blk .title {
		            height: auto!important
		        }

		        .main-blk .row-blk .prev {
		            margin-bottom: 20px
		        }

		        .main-blk .row-blk .prev img {
		            width: 100px;
		            vertical-align: top
		        }

		        .main-blk .row-blk h4 {
		            font-size: 20px;
		            color: #5c6373
		        }

		        .main-blk .row-blk span.red {
		            font-size: 20px;
		            color: #cc3d5e;
		            margin-top: 5px;
		            display: block
		        }

		        .main-blk {
		            background: #f8f9fa;
		            padding: 15px
		        }

		        .main-blk ul {
		            background: #fff;
		            margin: 0 -15px -15px;
		            padding: 15px 15px 20px
		        }

		        .main-blk ul li {
		            display: block;
		            position: relative;
		            padding-left: 15px;
		            color: #535457;
		            font-size: 16px;
		            margin-top: 10px;
		            line-height: 1.625;
		        }

		        .main-blk ul li:after {
		            position: absolute;
		            content: "";
		            left: 0;
		            top: 11px;
		            width: 6px;
		            height: 6px;
		            border-radius: 50%;
		            -webkit-border-radius: 50%;
		            -moz-border-radius: 50%;
		            background: #535457
		        }

		        .main-blk ul li:first-child {
		            margin-top: 0
		        }

		        .main-blk .number-list li {
		            font-size: 16px;
		            margin-top: 10px;
		            line-height: 1.625;
		            padding-left: 23px;
		        }

		        .main-blk .main-table {
		            border-spacing: 0;
		            border-collapse: separate;
		            width: 100%;
		            margin-top: 20px;
		            border-top: 2px solid #bec2cc
		        }

		        .main-blk .main-table th {
		            font-size: 16px;
		            color: #8e9199;
		            text-align: left;
		            font-weight: 400;
		            padding: 8px 12px;
		            border-bottom: 1px dotted #8e9199
		        }

				.main-blk .main-table td {
				    font-size: 16px;
				    line-height: 1.5;
				    border-bottom: 1px dotted #8e9199;
				    color: #5c6373;
				    padding: 12px 10px
				}

		        .main-blk.white {
		            background: #fff;
		            border-top: 1px solid #dfe1e6
		        }

		        .main-blk.white p:first-child {
		            padding-top: 15px
		        }

		        .main-blk a {
		            font-weight: 700;
		            font-size: 16px;
		            text-decoration: none;
		            color: #4684c4
		        }

		        .main-blk a:active {
		            text-decoration: underline
		        }

		        .main-blk .privacy-table {
		            display: block;
		            width: 100%;
		        }

		        .main-blk .main-table tbody{
		        	display: block;
		        	width: 100%;
		        }

		        .main-blk .privacy-table th {
		            display: none;
		        }

		        .main-blk .privacy-table tr {
		        	width: 100%;
		            display: -webkit-flex;
					display: -ms-flexbox;
					display: -ms-flex;
					display: flex;
					-webkit-flex-flow: column nowrap;
					-ms-flex-flow: column nowrap;
					flex-flow: column nowrap;
		            border-top: 2px solid #bec2cc;
		        }

		        .main-blk .privacy-table tr:first-child {
		            border-top: 0
		        }

		        .main-blk .privacy-table tr:not(:last-child) td:last-child {
		            border-bottom: 0;
		        }

		        .main-blk .privacy-table td {
		            display: block;
		            position: relative;
		            padding-left: 0;
		            padding-right: 0;
		            width: 100%;
		        }

		        .main-blk .privacy-table td span {
		            display: block;
		            width: 100%;
		            padding-left: 110px;
		        }

		        .main-blk .privacy-table td:before {
		            content: attr(data-title);
		            font-size: 13px;
		            color: #8e9199;
		            display: block;
		            width: 100px;
		            top: 15px;
		            position: absolute;
		            left: 0;
		            padding-right: 10px;
		        }

		        .account-blk .prev,
		        .account-blk .title {
		            display: inline-block;
		            vertical-align: middle
		        }

		        .main-blk p.silver {
		            text-align: center;
		            font-size: 12px;
		            color: #a6a9b3;
		            margin: -10px 0
		        }

		        .main-blk ul li:after{
					background: #535457;
				}

				.main-blk ul{
					background: transparent !important;
				}

				.main-blk .number-list li{
					padding-left: 20px;
				}

				.main-blk .number-list li:first-child{
					margin-top:10px;
				}

				.main-blk .scroll-links a[data-href],
				.main-blk .scroll-links span[data-href] {
					font-size: 16px;
					text-decoration: none;
					font-weight: bold;
					color: #4684c4;
					cursor: pointer;
				}

				.main-blk h4 {
					font-size: 20px;
					line-height: 1.3;
					color: #535457;
				}

				.main-blk section {
					padding-top: 30px;
				}

				.main-blk section:first-child {
					padding-top: 0;
				}

				.main-blk-content > section[id] {
					padding-top:30px;
				}

				.main-blk-content > section[id] h4 {
					padding-top: 20px;
				}

				.main-blk .number-list {
				    counter-reset: myCounter;
				}

				.main-blk .number-list li{
					display: block;
					position: relative;
				}

				.main-blk .number-list li:before {
				    position: absolute;
				    left: 0;
				    font-size: 16px;
				    top: 5px;
				    line-height: 1;
				    color: #535457;
				    display: inline-block;
				    counter-increment: myCounter;
				    content: counter(myCounter) ". ";
				}

				/*dark mode*/

		        .dark-mode body,
		        .dark-mode .main-blk,
		        .dark-mode .main-blk h2{
		            background: #1c1c1e;
		        }

		        .dark-mode .main-blk ul,
		        .dark-mode .main-blk.white,
		        .dark-mode .main-blk h3{
		            background: #141414;
		        }

		        .dark-mode .main-blk ul li:after{
		            background: #fff;
		        }

		        .dark-mode .main-blk p,
		        .dark-mode .main-blk ul li,
		        .dark-mode .main-blk h3,
		        .dark-mode .main-blk h2,
		        .dark-mode .main-blk .row-blk h4,
		        .dark-mode .main-blk h4,
		        .dark-mode .main-blk .number-list li:before,
		        .dark-mode .main-blk .privacy-table td:before,
		        .dark-mode .main-blk .main-table td{
		            color: #fff;
		        }

		        .dark-mode .main-blk p span.red,
		        .dark-mode .main-blk .row-blk span.red{
		            color: #0984FF;
		        }

		        .dark-mode .main-blk h2,
		        .dark-mode .main-blk h3,
		        .dark-mode .main-blk .privacy-table tr,
		        .dark-mode .main-blk.white,
		        .dark-mode .main-blk .main-table,
		        .dark-mode .main-blk .main-table th {
		            border-color: #5a5a5f;
		        }

		        .dark-mode .main-blk p.silver{
		            color: #8e8e93;
		        }

		        .dark-mode .main-blk .scroll-links a[data-href],
		        .dark-mode .main-blk .scroll-links span[data-href],
		        .dark-mode .main-blk a {
		            color: #0984FF;
		        }
			{% endif %}
		</style>
		<!-- end -->
		<div class="main-blk-content">
			<section>
				<h4>{{ 'intro.header'|trans|desc('Introduction') }}</h4>
				<p>{{ 'intro.pp_info'|trans|desc('This Privacy Policy will help you understand what information we collect and use at AwardWallet, and the choices you have associated with that information. When we refer to “AwardWallet,” “we,” “our,” or “us” in this policy, we are referring to AwardWallet, LLC which provides the Services to you. The “Services” refers to the applications, services, and websites (marketing and product) provided by AwardWallet. AwardWallet may, from time to time, introduce new products and services. To the extent that these new products and services affect this policy, we will notify you as described in Section 10 below. The use of information collected through our service shall be limited to the purpose of providing the Services for our customers and as specified herein.') }}</p>
				<p>
                    {{ 'intro.contact_info'|trans({
                        '%link_on%': '<a href="' ~ url('aw_contactus_index', queryExtend) ~ '">',
                        '%link_off%': '</a>',
                        '%link2_on%': '<a href="https://go.adr.org/dpf_irm.html" target="_blank" rel="noopener noreferrer">',
                        '%link2_off%': '</a>'
                    })|desc('If you have any questions about this privacy policy or our practices, please %link_on%contact us%link_off%. If you are from the European Economic Area (“EEA”) and you have an unresolved privacy or data use concern that we have not addressed satisfactorily, please contact our U.S.-based third party dispute resolution provider (free of charge) at %link2_on%https://go.adr.org/dpf_irm.html%link2_off%.')|raw }}
                </p>
			</section>
			<section>
				<h4>{{ 'contents.header'|trans|desc('Contents:') }}</h4>
				<ol class="number-list scroll-links">
					<li><span class="blue-link" data-href="#information-we-collect-and-receive"{{ contentLinkClick|raw }}>{{ 'data_collection_info.header'|trans|desc('Information We Collect and Receive') }}</span></li>
					<li><span class="blue-link" data-href="#use-information-we-collect-and-receive"{{ contentLinkClick|raw }}>{{ 'info_usage_policy.header'|trans|desc('How We Use the Information We Collect and Receive') }}</span></li>
					<li><span class="blue-link" data-href="#analytics-cookies-and-other-website-technologies"{{ contentLinkClick|raw }}>{{ 'web_tech_terms.header'|trans|desc('Analytics, Cookies and Other Web Site Technologies') }}</span></li>
					<li><span class="blue-link" data-href="#information-sharing"{{ contentLinkClick|raw }}>{{ 'info_sharing.header'|trans|desc('Information Sharing') }}</span></li>
					<li><span class="blue-link" data-href="#communicating"{{ contentLinkClick|raw }}>{{ 'communicating.header'|trans|desc('Communications') }}</span></li>
					<li><span class="blue-link" data-href="#accessing-your-data"{{ contentLinkClick|raw }}>{{ 'accessing_your_data.header'|trans|desc('Accessing Your Data') }}</span></li>
					<li><span class="blue-link" data-href="#california-privacy-rights"{{ contentLinkClick|raw }}>{{ 'california_privacy_rights.header'|trans|desc('California Consumer Privacy Act') }}</span></li>
					<li><span class="blue-link" data-href="#privacy-shield-compliance"{{ contentLinkClick|raw }}>{{ 'privacy_shield_compliance.header'|trans|desc('Data Privacy Framework Compliance') }}</span></li>
					<li><span class="blue-link" data-href="#security"{{ contentLinkClick|raw }}>{{ 'security.header'|trans|desc('Security') }}</span></li>
					<li><span class="blue-link" data-href="#changes-statement"{{ contentLinkClick|raw }}>{{ 'statement_changes.header'|trans|desc('Changes to this Statement') }}</span></li>
                    <li><span class="blue-link" data-href="#questions-or-complaints"{{ contentLinkClick|raw }}>{{ 'question_complaint.header'|trans|desc('Questions or Complaints') }}</span></li>
				</ol>
			</section>
			<section id="information-we-collect-and-receive">
				<h4>1. {{ 'data_collection_info.header'|trans|desc('Information We Collect and Receive') }}</h4>
				<p>{{ 'data_collection_info.1'|trans|desc('We collect several different types of information to provide Services to you, including:') }}</p>
				<p>
                    {{ 'data_collection_info.2'|trans({
                        '%bold_on%': '<strong>',
                        '%bold_off%': '</strong>'
                    })|desc('%bold_on%Customer Account and Registration Data:%bold_off% This includes information you provide to create your account with us and may include, first and last name, third party loyalty reward program information (which includes account access information), travel reservation information, a password and a valid email address.')|raw }}
                </p>
				<p>
                    <strong>
                        {{ 'data_collection_info.3'|trans|desc('Service Data (including Session and Usage data):') }}
                    </strong>
                </p>
				<p>{{ 'data_collection_info.4'|trans|desc('When you use our Services, we receive information generated through the use of the Service, or from the Service infrastructure itself, (for example, duration of session, connection information, etc.)  We may also collect usage and log data about how the services are accessed and used, including information about the device you are using the Services on, IP addresses, location information, language settings, what operating system you are using, unique device identifiers and other diagnostic data to help us support the Services. Please see Section 3 "Analytics, Cookies and other Website Technologies," for more information on Service Data.') }}</p>
				<p>
                    {{ 'data_collection_info.5'|trans({
                        '%bold_on%': '<strong>',
                        '%bold_off%': '</strong>'
                    })|desc('%bold_on%Third Party Data:%bold_off% We may receive information about you from other sources, including third party reward or loyalty programs (of which you have given us permission to access) publicly available databases or third parties from whom we have purchased data, and combine this data with information we already have about you. We may also receive information from other affiliated companies that are a part of our corporate group. This helps us to update, expand and analyze our records, identify new prospects for marketing, and provide products and services that may be of interest to you.')|raw }}
                </p>
                <p>
                    {{ 'data_collection_info.6'|trans({
                        '%bold_on%': '<strong>',
                        '%bold_off%': '</strong>'
                    })|desc('%bold_on%Location Information:%bold_off% We collect your location-based information and IP address for the purpose of providing and supporting the service and for fraud prevention and security monitoring.')|raw }}
                </p>
				<p>
                    {{ 'data_collection_info.7'|trans({
                        '%bold_on%': '<strong>',
                        '%bold_off%': '</strong>'
                    })|desc('%bold_on%Device Information:%bold_off% When you use our Services, we automatically collect information on the type of device you use, operating system version, and the unique device identifier (or "UDID").')|raw }}
                </p>
				<p>
                    {{ 'data_collection_info.8'|trans({
                        '%bold_on%': '<strong>',
                        '%bold_off%': '</strong>'
                    })|desc('%bold_on%Legal Basis for Processing (EEA Individuals Only):%bold_off% If you are from the EEA, our legal basis for collecting and using the personal information described above will depend on the personal information concerned and the specific context in which we collect it. If you are from the EEA, “personal information” is defined as any personally identifiable information or other data collected that could directly or indirectly identify you.')|raw }}
                </p>
				<p>{{ 'data_collection_info.9'|trans|desc('However, we will normally collect personal information from you where we have your consent to do so, where we need the personal information to perform Services, or where the processing is in our legitimate interests and not overridden by your data protection interests or fundamental rights and freedoms. In some cases, we may also have a legal obligation to collect the personal information in question.') }}</p>
				<p>{{ 'data_collection_info.10'|trans|desc('If we ask you to provide personal information to comply with a legal requirement or to provide you with Services, we will make this clear at the relevant time and advise you whether the provision of your personal information is mandatory or not (as well as of the possible consequences if you do not provide your personal information).') }}</p>
				<p>{{ 'data_collection_info.11'|trans|desc('Similarly, if we collect and use your personal information in reliance on our or a third party\'s legitimate interests and those interests are not already listed above, we will make clear to you at the relevant time what those legitimate interests are.') }}</p>
				<p>
                    {{ 'data_collection_info.12'|trans({
                        '%link_on%': '<a href="' ~ url('aw_contactus_index', queryExtend) ~ '">',
                        '%link_off%': '</a>'
                    })|desc('If you have questions about or need further information concerning the legal basis on which we collect and use your personal information, please %link_on%contact us%link_off%.')|raw }}</p>
			</section>
			<section id="use-information-we-collect-and-receive">
				<h4>2. {{ 'info_usage_policy.header'|trans|desc('How We Use the Information We Collect and Receive') }}</h4>
                <p>{{ 'info_usage_policy.1'|trans|desc('AwardWallet may access (which may include, with your consent, limited viewing or listening) and use the data we collect as necessary (a) to provide and maintain the Services; (b) to address and respond to service, security, and customer support issues; (c) to detect, prevent, or otherwise address fraud, security, unlawful, or technical issues; (d) as required by law; (e) to fulfill our contracts; (f) to improve and enhance the Services; (g) to provide analysis or valuable information back to our customers and users.') }}</p>
				<p>{{ 'info_usage_policy.2'|trans|desc('Some specific examples of how we use the information:') }}</p>
				<ul>
					<li>{{ 'info_usage_policy.2.1'|trans|desc('Create and administer your account') }}</li>
					<li>{{ 'info_usage_policy.2.2'|trans|desc('Send you an order confirmation') }}</li>
					<li>{{ 'info_usage_policy.2.3'|trans|desc('Facilitate and improve the usage of the services you have ordered') }}</li>
					<li>{{ 'info_usage_policy.2.4'|trans|desc('Send you product updates, marketing communication, and service information') }}</li>
					<li>{{ 'info_usage_policy.2.5'|trans|desc('Respond to customer inquiries and support requests') }}</li>
					<li>{{ 'info_usage_policy.2.6'|trans|desc('Conduct research and analysis') }}</li>
					<li>{{ 'info_usage_policy.2.7'|trans|desc('Display content based upon your interests') }}</li>
