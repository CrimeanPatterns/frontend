{% set MAIN_BODY_CLASSES = {
    'landing': true,
    'not-logged': false,
    'business': is_granted('SITE_BUSINESS_AREA'),
    'manual-hidden': false,
    'responsive': false,
    'hide-menu': false
}|merge(MAIN_BODY_CLASSES|default({})) %}

{% extends "@AwardWalletMain/Layout/not_logged.html.twig" %}
{% import "@AwardWalletMain/TwigMacros/recaptcha.html.twig" as recaptcha %}

{% block title %}
    {% if currentRoute in ['aw_login', 'aw_login_locale'] %}
        {{ 'meta.title.login'|trans|desc('Log in to AwardWallet account') }}
    {% elseif currentRoute in ['aw_register', 'aw_register_locale'] %}
        {{ 'meta.title.register'|trans|desc('Create your AwardWallet account') }}
    {% else %}
        {{ 'meta.title'|trans|desc('Track frequent flyer reward programs and plan your travel') }}
    {% endif %}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script type="text/javascript">
        if (!!navigator.userAgent) {
            if (/iPad|iPhone|iPod/i.test(navigator.userAgent)) {
                document.body.classList.add('is-device-ios');
            } else if (/android/i.test(navigator.userAgent.toLowerCase())) {
                document.body.classList.add('is-device-android');
            }
        }

        {% if invite is defined and invite %}
        {% set invitee = invite.familyMember ? invite.familyMember : invite.inviteeid %}
        window.inviteEmail = {{ invite.email ? invite.email|json_encode|raw : '""' }};
        window.firstName = {{ invitee ? invitee.firstname|capitalize|json_encode|raw : '""' }};
        window.lastName = {{ invitee ? invitee.lastname|capitalize|json_encode|raw : '""' }};
        window.inviteCode = {{ invite.code|json_encode|raw }};
        {% endif %}
        var locale = {{ app.request.locale | json_encode | raw }};
        require(['jquery-boot', 'vendor/lazysizes/lazysizes'], function ($) {
            $(function () {
                if (!!navigator.userAgent) {
                    if (/iPad|iPhone|iPod/i.test(navigator.userAgent)) {
                        document.getElementById('pageWrap').classList.add('is-device-ios');
                    } else if (/android/i.test(navigator.userAgent.toLowerCase())) {
                        document.getElementById('pageWrap').classList.add('is-device-android');
                    }
                }
                $('.app ').css('zoom', ((1024 / $(window).width()) * 100) + '%' );

                require(['lib/design', 'pages/landing/main']);
            });
        });
        {#
        {% if isMobileDevice() %}
        require(['lib/design']);
        document.getElementById('merchant').addEventListener('focus', (event) => require(['pages/landing/main']));
        {% else %}
        require(['pages/landing/main']);
        {% endif %}
        #}
    </script>

    {% if load_external_scripts %}
        <script type="text/javascript">
            window.fbAsyncInit = function() {
                FB.init({
                    appId            : '191119281484667',
                    autoLogAppEvents : true,
                    xfbml            : true,
                    version          : 'v2.11'
                });
                FB.AppEvents.logPageView();
            };
            (function(d, s, id) {
                var js, fjs = d.getElementsByTagName(s)[0];
                if (d.getElementById(id)) return;
                js     = d.createElement(s);
                js.id  = id;
                js.async = 1;
                js.src = 'https://connect.facebook.net/en_US/sdk/xfbml.group.js#xfbml=1&version=v2.11&appId=191119281484667';
                fjs.parentNode.insertBefore(js, fjs);
            }(document, 'script', 'facebook-jssdk'));
        </script>
    {% endif %}
{% endblock %}

{% block logo_attr %}ui-sref="home"{% endblock %}

{% block meta %}
    {{ parent() }}
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, viewport-fit=cover">
    <meta name="keywords" content="{{ 'meta.keywords'|trans|desc('Incentive reward center, Marriott reward, cheap airfare, reward incentive program, American Express rewards, trip reward, air miles reward program, frequent flyer miles, track award program balances, air mile reward, award software, award program, delta sky miles, reward program reward network') }}">
    <meta name="description" content="{{ 'meta.description'|trans|desc('AwardWallet helps you track frequent flyer miles and hotel points as well as book reward tickets') }}">
    <meta name="google-site-verification" content="rtczCsWSE3xSyZ3TiyI6quRz2fgkTgAEgCVJvQVn0R0" />
    <base href="{{ app.request.pathInfo | raw }}">
{% endblock %}

{% block register_button_options %}ui-sref="register" ui-sref-active="active"{% endblock %}
{% block register_business_button_options %}ui-sref="registerBusiness" ui-sref-active="active"{% endblock %}
{% block login_button_options %}ui-sref="login" ui-sref-active="active"{% endblock %}

