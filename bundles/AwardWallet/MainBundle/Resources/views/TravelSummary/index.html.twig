{% set MOBILE_NAV = true %}
{% set HIDE_NAV = true %}
{% set MAIN_BODY_CLASSES = {
    'manual-hidden': true,
    'responsive': true,
    'hide-menu': true,
    'hide-env-content': isBlogUserSummary is same as(true),
    'blogTimelineSummary': isBlogUserSummary is same as(true),
}|merge(MAIN_BODY_CLASSES|default({})) %}

{% extends "@AwardWalletMain/Layout/common.html.twig" %}

{% block meta %}
    {{ parent() }}
    <meta name="viewport"
          content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, viewport-fit=cover">
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="https://unpkg.com/@google/markerclustererplus@4.0.1/dist/markerclustererplus.min.js"></script>
    <script type="text/javascript">
        {% if isBlogUserSummary is same as(true) %}
        {{ jsTest1|raw }}
        {% endif %}
        function initFirstLoad() {
            require([
                'jquery-boot',
                'angular-boot',
                'pages/travelSummary',
                'lib/design'
            ], function ($, angular) {
                angular.module('travelSummaryApp').constant('data', {{ data|json_encode|default('false')|raw }});
                angular.module('travelSummaryApp').constant('statTabs', {
                    airports: '{{ translations.airports }}',
                    countries: '{{ translations.countries }}',
                    airlines: '{{ translations.airlines }}'
                });
                $(function () {
                    angular.bootstrap($('div.content').get(), ['travelSummaryApp']);
                    TravelSummary.init();
                    {% if isBlogUserSummary is same as(false) %}
                    TravelSummary.showMailboxPopup();
                    {% else %}
                    $('[data-tip]', '.blogTimelineSummary').removeAttr('data-tip');
                    $('[title]', '.blogTimelineSummary').removeAttr('title');
                    {{ jsTest2|raw }}
                    window.callFromBlog = function(params) {
                        const htmlEl = document.querySelector('html');
                        if ('' === params || ('light' !== params.theme && 'dark' !== params.theme)) {
                            htmlEl.classList.remove('light-mode');
                            htmlEl.classList.remove('dark-mode');
                        } else if ('light' === params.theme) {
                            htmlEl.classList.remove('dark-mode');
                            htmlEl.classList.add('light-mode');
                        } else if ('dark' === params.theme) {
                            htmlEl.classList.remove('light-mode');
                            htmlEl.classList.add('dark-mode');
                        }
                    }
                    $(window).resize(function(){
                        window.parent.postMessage({
                            func: 'authorSummaryInfo',
                            height: document.getElementById('body').scrollHeight,
                            distance: '{{ data.distance }}',
                            countries: '{{ data.locationStat.getCountries }}',
                            travelYear: '{{ data.travelYear }}',
                        }, '*');
                    });
                    $(window).resize();
                    {% endif %}
                });
            });
        }
    </script>
    <script async defer
            src="https://maps.googleapis.com/maps/api/js?key={{ googleKey }}&callback=initFirstLoad">
    </script>
    <style>
        .popup-container {
            cursor: auto;
            height: 0;
            position: absolute;
            /* The max width of the info window.
            width: 200px; */
        }
    </style>
{% endblock %}

{% block content %}
    <div data-ng-controller="mainCtrl as main" data-ng-cloak>
        <div class="main-blk silver empty">
            {% if isBlogUserSummary is same as(true) %}
                <h1>Travel Summary for {{ data.travelYear }}</h1>
            {% else %}
            <h1>{{ translations.historicalTravelSummary }}</h1>
            <div class="main-blk-actions">
                <div>
                    <label for="traveler">{{ translations.traveler }}:</label>
                    <div class="styled-select middle">
                        <div>
                            <select data-ng-model="main.state.currentUser" data-ng-change="main.handleChange()" id="traveler">
                                {% for id, name in agents %}
                                    <option value="{{ id }}">{{ name }}</option>
                                {% endfor %}
                            </select>
                        </div>
                    </div>
                </div>
                <div>
                    <label for="timeframe">{{ translations.timeframe }}:</label>
                    <div class="styled-select middle">
                        <div>
                            <select data-ng-model="main.currentPeriod" id="timeframe" data-ng-model-options="{getterSetter: true}">
                                {% for key, period in periods %}
                                <option value="{{ key }}">{{ period }}</option>
                                {% endfor %}
                            </select>
                        </div>
                    </div>
                </div>
            </div>
            {% endif %}
        </div>
        <div class="main-blk" data-ng-show="main.state.isLoading">
            <div class="ajax-loader">
                <div class="loading"></div>
            </div>
        </div>
        <div class="main-blk" data-ng-show="main.state.noData && !main.state.isLoading">
            <div class="not-found" style="border: 0px;">
                <i class="icon-warning-small"></i>
                <p>{{ translations.noData | raw }}</p>
            </div>
{#            <div class="ajax-loader">#}
{#                <h2>No Data</h2>#}
{#            </div>#}
        </div>

        <div id="map-modal" class="map-modal-container" data-ng-show="main.state.showMapPopup">
            <div class="map-modal-header">
                <div class="map-modal-title">
                    <span class="[[ main.state.category ]]"></span>
                    <div class="location" ng-switch on="main.state.category">
                        <h3 ng-switch-when="air">[[ main.state.activeAirport.key ]] <span>([[ main.state.activeAirport.title ]])</span></h3>
                        <h3 ng-switch-when="hotel|parking|rental|restaurant|meeting|show|event|conference|rave" ng-switch-when-separator="|" data-ng-if="!main.state.activeAirport.payload.differentTitles">[[ main.state.activeAirport.key ]]</h3>
                        <h3 ng-switch-when="hotel|parking|rental|restaurant|meeting|show|event|conference|rave" ng-switch-when-separator="|" data-ng-if="main.state.activeAirport.payload.differentTitles">[[ main.state.activeAirport.payload.address ]]</h3>
                        <span class="extended" ng-switch-when="hotel|parking|rental|restaurant|meeting|show|event|conference|rave" ng-switch-when-separator="|">[[ main.state.activeAirport.title ]]</span>
                        <h3 ng-switch-when="bus|train|cruise|ferry|transfer" ng-switch-when-separator="|">[[ main.state.activeAirport.title ]]</h3>
                    </div>
                </div>
                <button class="map-modal-button" data-ng-click="main.handlePopupClose($event)"><span class="icon-close"></span></button>
            </div>
            <div class="map-modal-body">
                <div class="map-modal-table">
                    <a class="row" href="[[ segment.timelineLink ]]" target="_blank" data-ng-if="main.state.airport" data-ng-repeat="(index, segment) in main.state.activeAirport.payload.segments track by index">
                        <div class="cell row-date-cell"><div data-ng-bind="segment.depDate"></div></div>
                        <div class="cell">
                            <div>
                                <span class="aircode" data-ng-bind="segment.depCode"></span>
                                <span class="route-arrow"></span>
                                <span class="aircode" data-ng-bind="segment.arrCode"></span>
                            </div>
                        </div>
                        <div class="cell row-travel-plan-cell" data-ng-bind="segment.travelPlan"></div>
                    </a>

                    <a class="row" href="[[ segment.timelineLink ]]" target="_blank" data-ng-if="!main.state.airport" ng-switch on="main.state.category" data-ng-repeat="(index, segment) in main.state.activeAirport.payload.segments track by index">
                        <div class="cell row-date-cell"><div data-ng-bind="segment.date"></div></div>
                        <div class="cell" ng-switch-when="hotel|parking|rental|restaurant|meeting|show|event|conference|rave" ng-switch-when-separator="|" data-ng-bind="main.getDurationText(main.state.category, segment)"></div>
                        <div class="cell" ng-switch-when="hotel|parking|rental|restaurant|meeting|show|event|conference|rave" ng-switch-when-separator="|" data-ng-if="main.state.activeAirport.payload.differentTitles" data-ng-bind="segment.details"></div>
                        <div class="cell" ng-switch-when="bus|train|cruise|ferry|transfer" ng-switch-when-separator="|" data-ng-bind="main.state.activeAirport.payload.reservationTitle"></div>
                        <div class="cell row-travel-plan-cell" data-ng-bind="segment.travelPlan"></div>
                    </a>
                </div>
            </div>
        </div>

