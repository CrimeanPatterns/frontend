{% set MOBILE_NAV = true %}
{% set HIDE_NAV = true %}
{% set MAIN_BODY_CLASSES = {
    'manual-hidden': true,
    'responsive': true,
    'hide-menu': true
}|merge(MAIN_BODY_CLASSES|default({})) %}

{% extends app.user is not null ? "@AwardWalletMain/Layout/common.html.twig" : "@AwardWalletMain/Layout/not_logged.html.twig" %}

{% block title %}{{ 'page.aboutus.title'|trans }}{% endblock %}

{% block meta %}
    {{ parent() }}
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, viewport-fit=cover">
    <meta name="description" content="{{ 'technology-company-loyalty-industry'|trans|desc('AwardWallet is a technology company in the loyalty space. We developed a tool for tracking points and miles. Learn more about us.') }}">
{% endblock %}

{% block content %}
    <div class="static-page" id="about-us-page">
        {% include "@AwardWalletMain/SiteInfo/aboutUs.html.twig" with {
            'programs': programs,
            'members' : members,
            'miles'   : miles,
            'money'   : money,
            'headMain': 'about-awardwallet'|trans|desc('About Us')
        } %}
    </div>
{% endblock %}
