{% macro requestListClassRow(request, isBooker) %}{% apply spaceless %}
    {% set status = request.getRealStatus(isBooker) %}
    {% if status == constant('\\AwardWallet\\MainBundle\\Entity\\AbRequest::BOOKING_STATUS_NOT_VERIFIED') %}
        not verified
    {% elseif status == constant('\\AwardWallet\\MainBundle\\Entity\\AbRequest::BOOKING_STATUS_PENDING') %}
        opened
    {% elseif status == constant('\\AwardWallet\\MainBundle\\Entity\\AbRequest::BOOKING_STATUS_PROCESSING') %}
        paid
    {% elseif status == constant('\\AwardWallet\\MainBundle\\Entity\\AbRequest::BOOKING_STATUS_BOOKED') %}
        booked
    {% elseif status == constant('\\AwardWallet\\MainBundle\\Entity\\AbRequest::BOOKING_STATUS_CANCELED') %}
        canceled
    {% elseif status == constant('\\AwardWallet\\MainBundle\\Entity\\AbRequest::BOOKING_STATUS_FUTURE') %}
        future
    {% endif %}
{% endapply %}{% endmacro %}

