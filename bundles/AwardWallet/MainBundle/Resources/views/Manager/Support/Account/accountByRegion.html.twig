{% set title = "Search Accounts" %}
{% set contentTitle = "Search accounts" %}
{% use '@AwardWalletMain/FormTheme/oldFields.html.twig' %}
{% extends '@AwardWalletMain/Manager/layout.html.twig' %}
{% block content %}
    <style>
        a.disabled {
            pointer-events: none;
            cursor: default;
            color: grey;
        }
        .box {
            float: left;
            margin-right: 3px;
            height: 40px;
        }
        select {
            margin-top: 7px;
        }
        input {
            margin-top: 7px;
            border-width: 1px;
        }
        td {
            vertical-align: text-top;
        }

        table.tableRes td.checkdate {
            width: 140px;
        }

        table.tableRes td.account {
            width: 110px;
        }

        table.tableRes th.message {
            /*width: 310px;*/
            font-size: inherit;
        }

        table.tableRes th {
            background-color: whitesmoke;
            font-weight: bold;
            border-top: #D0D0D0 1px solid;
            border-bottom: #D0D0D0 1px solid;
            text-align: center;
            padding: 5px;
        }

        table.tableRes td.check {
            text-align: center;
        }

        table.tableRes td {
            padding: 4px;
        }

        table.tableRes {
            margin-top: 7px;
            border-width: 0 !important;
            vertical-align: middle;
        }

        table.tableRes .bothBorder {
            border-left: #D0D0D0 1px solid;
            border-right: #D0D0D0 1px solid;
        }

        table.tableRes .leftBorder {
            border-left: #D0D0D0 1px solid;
        }

        table.tableRes .rightBorder {
            border-right: #D0D0D0 1px solid;
        }

        table.tableRes .topBorder {
            border-top: #D0D0D0 1px solid;
        }

        table.tableRes .bottomBorder {
            border-bottom: #D0D0D0 1px solid;
        }
    </style>

    <script>
        function checkAccounts() {
            ids = [];
            $('.checkAcc').find('input:checked').parent().parent().parent().find('td.account span').each(function(){ids.push($(this).get(0).innerText);});
            if( ids.length !== 0 ){
                window.open("/manager/checkAccount.php?ID="+ids.join(), "_self");
            }
        }
        function selectAll() {
            $('.checkAcc').find('input').prop('checked', true).parent().parent().parent().css('background-color', 'lavender');
        }

        function unselect() {
            $('.checkAcc').find('input').prop('checked', false).parent().parent().parent().css('background-color', '');
        }

        function setColor(el) {
            if ($(el).attr('checked'))
                $(el).parent().parent().parent().css('background-color', 'lavender');
            else
                $(el).parent().parent().parent().css('background-color', '');
        }
        function getRegion(code) {
            $.ajax({
                url: '{{ path('aw_manager_accountByRegion_login') }}',
                type: "POST",
                data: {id: code},
                async: false,
                success: function (data) {
                    $('input[name="ignoreLogin"]').get(0).checked = false;
                    if (data.data) {
                        $('#checkIgnoreLogin').show();
                    } else {
                        $('#checkIgnoreLogin').hide();
                    }
                    $('#region').html(data.data);
                    if (data.javascript) {
                        require(['pages/support/desktopForm'],
                            /**
                             * @param {DesktopForm} desktopForm
                             */
                            function (desktopForm) {
                                var form = $('form[name=s]');
                                (function () {
                                    //  form logic customizations
                                    eval(data.javascript);
                                    desktopForm.init(addFormExtension);
                                })();
                            });
                    }
                }
            });
        }
        function run() {
            var l2 = $("#login2:visible");
            if (l2.length > 0) {
                if (l2.find('option:selected').length > 0)
                    $('input[name="login2"]').val(l2.find('option:selected').val());
                else
                    $('input[name="login2"]').val(l2.val());
            } else
                $('input[name="login2"]').val('');
            var l3 = $("#login3:visible");
            if (l3.length > 0) {
                if (l3.find('option:selected').length > 0)
                    $('input[name="login3"]').val(l3.find('option:selected').val());
                else
                    $('input[name="login3"]').val(l3.val());
            } else
                $('input[name="login3"]').val('');

            $('form').submit();
        }
        function copy(e) {
            var text = e.previousElementSibling.innerText;
            var input = document.createElement('input');
            document.body.appendChild(input);
            input.value = text;
            input.select();
            document.execCommand("copy");
            document.body.removeChild(input);
        }
        $(document).ready(function () {
            getRegion(document.forms['s'].elements['providerID'].value);
            var l2 = $("#login2");
            {% if inputValue.login2 is defined and not inputValue.login2 is empty and inputValue.ignoreLogin!='on' %}
            if (l2.find('option').length > 0)
                l2.find('option[value="{{ inputValue.login2 }}"]').prop('selected', true);
            else
                l2.val("{{ inputValue.login2 }}");
            {% endif %}
            var l3 = $("#login3");
            {% if inputValue.login3 is defined and  not inputValue.login3 is empty %}
            if (l3.find('option').length > 0)
                l3.find('option[value="{{ inputValue.login3 }}"]').prop('selected', true);
            else
                l3.val("{{ inputValue.login3 }}");
            {% endif %}
            if (window.location.search.length > 0) {
                var txt;
                {% if not data is empty %}
                txt = 'Result for ';
                {% else %}
                txt = 'Empty result for ';
                {% endif %}
                txt += '<b>' + $("select[name='UnusedID']").find('option:selected').get(0).innerText
                    + ' / '
                    + $("select[name='providerID']").find('option:selected').get(0).innerText
                    + '</b>'
                ;
                {% if inputValue.ignoreLogin!='on' %}

                if (l2.find('option:selected').length > 0 && (l2.find('option:selected').get(0).innerText.search(/^Select /)!==0))
                    txt += ' login2: ' + '<b>' + l2.find('option:selected').get(0).innerText + '</b>';
                else if (l2.length > 0 && l2.val().length > 0)
                    txt += ' login2: ' + '<b>' + l2.val() + '</b>';
                if (l3.find('option:selected').length > 0 && (l3.find('option:selected').get(0).innerText.search(/^Select /)!==0))
                    txt += ' login3: ' + '<b>' + l3.find('option:selected').get(0).innerText + '</b>';
                else if (l3.length > 0 && l3.val().length > 0)
                    txt += ' login3: ' + '<b>' + l3.val() + '</b>';
                {% endif %}
                {% if not data is empty %}
                txt += '<span>&nbsp;-&nbsp;total accounts (password With AwardWallet.com): <b>{{ dataCount }}</b></span>';
                {% endif %}
                {% if inputValue.providerID is defined %}
                txt += '<a target="_blank" href="/manager/operations?ID={{ inputValue.providerID }}" style="margin-left:7px; color:lightgray; text-decoration:none;" title="Operations"><img src="data:image/png;base64," width="23" height="20" alt="  "></a>';
                {% endif %}
                $('#hResult').html(txt);
                $('#hResult').show();
            } else
                $('#hResult').html('');
            let _tableRows = $('.tableRes tbody tr');
            _tableRows.each(function(i) {
                $(this).find('td:first').text(i);
            });

        });

    </script>

    <form method="get" name="s">
        <input type="hidden" name="search" value="1">
        <input type="hidden" name="login2" value="">
        <input type="hidden" name="login3" value="">
        <div style="margin-top: 5px;">Select Provider:</div>
        <select name="UnusedID"
                onchange="document.forms['s'].elements['providerID'].value=this.value;getRegion(this.value);">
            {% for row in providerName %}
                <option value='{{ row.ID }}' {% if inputValue.providerID is defined and row.ID==inputValue.providerID %} selected {% endif %}>{{ row.Description }}</option>
            {% endfor %}
        </select>
        <select style="width: 200px;" name="providerID"
                onchange="document.forms['s'].elements['UnusedID'].value=this.value;getRegion(this.value);">
            {% for row in providerCode %}
                <option value='{{ row.ID }}' {% if inputValue.providerID is defined and row.ID==inputValue.providerID %} selected {% endif %}>{{ row.Description }}</option>
            {% endfor %}
        </select>
        </br>
        </br>
        <div class="box">
        <div id="region" style="display: flex"></div>
        </div>
        <div class="box">
        <div>Error Code:</div>
        <select style="width: 200px;" name="errorCode">
            {% for row in errorCode %}
                <option value='{{ row.ID }}' {% if row.ID==inputValue.errorCode %} selected {% endif %}>{{ row.Description }}</option>
            {% endfor %}
        </select>
        </div>
        <div id="checkIgnoreLogin" class="box" style="display: none">
        <div>&nbsp;</div>
            <label title="consider only ErrorCode and Provider"><input style="margin-top: 8px;" type="checkbox" name="ignoreLogin" {% if inputValue.ignoreLogin=='on' %}checked{% endif %}> Only ErrorCode</label>
        </div>
        <div class="box" style="position: relative;">
        <button style="margin-left: 50px; position: absolute; bottom: -2px;" onclick="run();">Search</button>
        </div>

    </form>
    </br>
    </br>
    </br>
    </br>
    <span id="hResult" style="display: none">Result for </span>
    <table class="tableRes" cellspacing="0" cellpadding="0">
        {% if not data is empty %}

            <tr>
                <th class="nn bothBorder leftBorder">#</th>
                <th class="updatedate rightBorder">Update Date</th>
                <th class="checkdate rightBorder">Success Check</th>
                <th class="account rightBorder">AccountID</th>
                <th class="user rightBorder">UserId</th>
                <th class="login rightBorder">Login</th>
                <th class="login2 rightBorder">Login2</th>
                <th class="login3 rightBorder">Login3</th>
                <th class="balance rightBorder">Balance</th>
                <th class="messageH rightBorder">Error Message</th>
                <th class="options rightBorder">Options</th>
                <th class="check rightBorder"><p style="text-align: right; margin: 0; padding: 0"><span><a id="selectAll" href="javascript:selectAll()">Select all</a> / <a
                                    id="unselect" href="javascript:unselect()">Unselect</a></span></p>
                    <a href="javascript:checkAccounts()" {% if inputValue.HideCheck is defined and inputValue.HideCheck %}class="disabled"{% endif %} >Check selected</a></th>
            </tr>
        {% endif %}
        {% for row in data %}
            <tr>
                <td class="nn rightBorder bottomBorder leftBorder"></td>
                <td class="updatedate rightBorder bottomBorder">{{ row.UpdateDate|date("m/d/Y H:i:s") }}</td>
                <td class="checkdate rightBorder bottomBorder">{{ row.SuccessCheckDate|date("m/d/Y H:i:s") }}</td>
                <td class="account rightBorder bottomBorder" nowrap>
                    <span>{{ row.AccountID }}</span>
                    <button style='float:right;padding:0;background:none;border:none;color:gray;'
                            onclick="copy(this)">&nbsp; Copy
                    </button>
                </td>
                <td class="user rightBorder bottomBorder">{{ row.UserID }}</td>
                <td class="login rightBorder bottomBorder">{{ row.Login }}</td>
                <td class="login2 rightBorder bottomBorder">{{ row.Login2 }}</td>
                <td class="login3 rightBorder bottomBorder">{{ row.Login3 }}</td>
                <td class="balance rightBorder bottomBorder">{{ row.Balance }}</td>
                <td class="message rightBorder bottomBorder">{{ row.ErrorMessage }}</td>
                <td class="options rightBorder bottomBorder" nowrap>
                    <a href="{{ path('aw_manager_impersonate') }}?UserID={{ row.UserID }}&AutoSubmit&AwPlus=1"
                       target="_blank">Impersonate</a>
                    | <a href="/manager/passwordVault/requestPassword.php?ID={{ row.AccountID }}"
                       target="_blank">Request pass</a>
                    | <a href="{{ path('aw_manager_loyalty_logs') }}?AccountID={{ row.AccountID }}"
                       target="_blank">Logs</a>
                    | <a href="/manager/checkAccount.php?ID={{ row.AccountID }}"{% if inputValue.HideCheck is defined and inputValue.HideCheck %}class="disabled"{% endif %}>Check</a>
                </td>
                <td class="checkAcc rightBorder bottomBorder" style="text-align: center">
                    <label style="padding: 7px 25px 7px;">
                        <input type="checkbox" onclick="setColor(this);"/>
                    </label>
                </td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}