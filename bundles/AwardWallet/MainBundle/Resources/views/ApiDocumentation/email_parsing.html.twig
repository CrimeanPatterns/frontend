      <span class="nt">"pricingInfo"</span></span><span class="p">:</span><span class="w"> </span><span class="s2"><span class="p">{</span></span>
        <span class="nt">"total"</span></span><span class="p">:</span><span class="w"> </span><span class="s2">251.41<span class="p">,</span></span>
        <span class="nt">"cost"</span></span><span class="p">:</span><span class="w"> </span><span class="s2">193.75<span class="p">,</span></span>
        <span class="nt">"discount"</span></span><span class="p">:</span><span class="w"> </span><span class="s2">40<span class="p">,</span></span>
        <span class="nt">"spentAwards"</span></span><span class="p">:</span><span class="w"> </span><span class="s2">"10000 points"</span><span class="p">,</span></span>
        <span class="nt">"currencyCode"</span></span><span class="p">:</span><span class="w"> </span><span class="s2">"USD"</span><span class="p">,</span></span>
        <span class="nt">"fees"</span></span><span class="p">:</span><span class="w"> </span><span class="s2"><span class="p">[</span></span>
          <span class="p">{</span></span>
            <span class="nt">"name"</span></span><span class="p">:</span><span class="w"> </span><span class="s2">"Tax"</span><span class="p">,</span></span>
            <span class="nt">"charge"</span></span><span class="p">:</span><span class="w"> </span><span class="s2">34.56
          <span class="p">}</span><span class="p">,</span></span>
          <span class="p">{</span></span>
            <span class="nt">"name"</span></span><span class="p">:</span><span class="w"> </span><span class="s2">"Insurance"</span><span class="p">,</span></span>
            <span class="nt">"charge"</span></span><span class="p">:</span><span class="w"> </span><span class="s2">23.1
          <span class="p">}</span>
        <span class="p">]</span>
