<span class="p">{</span></span>
  <span class="nt">"userData"</span></span><span class="p">:</span><span class="w"> </span><span class="s2">"some new user data"</span><span class="p">,</span></span>
  <span class="nt">"tags"</span></span><span class="p">:</span><span class="w"> </span><span class="s2"><span class="p">[</span></span>
    <span class="nt">"test2"</span><span class="p">,</span></span>
    <span class="nt">"userid_1236"</span>
  <span class="p">]</span><span class="p">,</span></span>
  <span class="nt">"onProgress"</span></span><span class="p">:</span><span class="w"> </span><span class="s2"><span class="p">{</span></span>
    <span class="nt">"callbackUrl"</span></span><span class="p">:</span><span class="w"> </span><span class="s2">"https://some.example.host/new-progress-url"</span>
  <span class="p">}</span><span class="p">,</span></span>
  <span class="nt">"access_token"</span></span><span class="p">:</span><span class="w"> </span><span class="s2">"xxx"</span><span class="p">,</span></span>
  <span class="nt">"referesh_token"</span></span><span class="p">:</span><span class="w"> </span><span class="s2">"yyy"</span><span class="p">,</span></span>
  <span class="nt">"returnEmail"</span></span><span class="p">:</span><span class="w"> </span><span class="s2">"none"</span>
<span class="p">}</span>
                            </code>
                        </pre>
                                                                <h4 id="http-request-email_scanner_11">Request</h4>
                                                    <p><code class="prettyprint">PUT https://service.awardwallet.com/email/json/v2/mailboxes/{mailboxId}</code></p>
                                                    <p><code class="prettyprint">PUT https://email-eu.awardwallet.com/email/json/v2/mailboxes/{mailboxId}</code></p>
                                                                                                                                        <h4 id="query-parameters-email_scanner_11">Request Parameters</h4>
