<span class="p">{</span></span>
  <span class="nt">"email"</span></span><span class="p">:</span><span class="w"> </span><span class="s2">"john@gmail.com"</span><span class="p">,</span></span>
  <span class="nt">"accessToken"</span></span><span class="p">:</span><span class="w"> </span><span class="s2">"abcd"</span><span class="p">,</span></span>
