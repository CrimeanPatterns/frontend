  <span class="nt">"userData"</span></span><span class="p">:</span><span class="w"> </span><span class="s2">"blah"</span><span class="p">,</span></span>
  <span class="nt">"browserState"</span></span><span class="p">:</span><span class="w"> </span><span class="s2">"v1:YZFsNRWtg7..."</span><span class="p">,</span></span>
